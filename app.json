{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "potrait",
    "icon": "app/assets/img/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "app/assets/img/android-splash-preview-dark.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "app/assets/img/android-splash-preview2.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "app/assets/img/favicon.png"
    }
  }
}
